pico-8 cartridge // http://www.pico-8.com
version 0
__lua__
t=0
function _init()
 cls()
end

function _update()
 t+=1
end

function _draw()
 cls(1)
 for i=0,15 do
  rectfill(i*8,0,i*8+7,7,i)
 end
 print("spark fantasy console",10,40,7)
 print("a pico-8 clone",28,50,6)
 circ(64,90,10+sin(t/30)*5,10)
 circfill(64,90,6+sin(t/30)*3,12)
 for i=0,5 do
  local x=64+cos(t/60+i/6)*30
  local y=90+sin(t/60+i/6)*20
  pset(x,y,8+i)
 end
end
